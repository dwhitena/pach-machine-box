{
  "pipeline": {
    "name": "tag"
  },
  "transform": {
    "image": "dwhitena/tagimage",
    "cmd": [ "/tagimage" ]
  },
  "parallelism_spec": {
    "strategy": "CONSTANT",
    "constant": "1"
  },
  "inputs": [
    {
      "repo": {
        "name": "unidentified"
      },
      "glob": "/"
    },
    {
      "repo": {
        "name": "tags"
      },
      "glob": "/"
    },
    {
      "repo": {
        "name": "identify"
      },
      "glob": "/*"
    }
  ]
}
